name: "mnist_siamese_train_test"

layer {
  name: "data"
  type: "Python"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  python_param {
    module: "src.data_layer.ft_layer_live"
    layer: "DataLayer"
    #batch_size: 160
    param_str: " {\'pascal_root\': \'data\' ,\'split\': \'ft_live_train\', \'im_shape\': [224, 224],\'batch_size\': 30}"
  }
}
layer {
  name: "data"
