# This is for testing, it's run every thousand iterations or so.
layer {
  include { phase: TEST }
  name: "accuracy"
  type: "Python"
  bottom: "fc8"
  bottom: "label"
  top: "spearmanr"
  top: "pearsonr"

  python_param {
    module: 'src.tools.layers_ft'
    layer: 'AngularErrorLayer'
  }
}


layer {
  name: "loss2"
